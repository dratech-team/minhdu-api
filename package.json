{
  "name": "minhdu-api",
  "version": "0.0.1",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "description": "Dratech - Giải pháp thiết kế phần mềm tối ưu",
  "author": "DRATECH",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nest start --watch",
    "start:debug:webstorm": "node %NODE_DEBUG_OPTION% -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "NODE_ENV=production node dist/api/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "push": "prisma db push",
    "docs": "npx @compodoc/compodoc -p tsconfig.json -s",
    "seed": "npx prisma db seed"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "7.6.15",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "7.6.15",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/mapped-types": "0.4.1",
    "@nestjs/passport": "7.1.6",
    "@nestjs/platform-express": "7.6.15",
    "@nestjs/schedule": "0.4.3",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/websockets": "7.6.17",
    "@prisma/client": "3.7.0",
    "@types/lodash": "4.14.174",
    "@types/mongoose-paginate": "5.0.8",
    "@types/passport": "1.0.6",
    "basic-auth-token": "0.4.2",
    "bcrypt": "5.0.1",
    "carrier": "0.3.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "compression": "1.7.4",
    "date-fns": "2.28.0",
    "exceljs": "4.3.0",
    "express": "4.17.1",
    "express-rate-limit": "5.2.6",
    "faker": "5.5.3",
    "formidable": "1.2.6",
    "helmet": "4.4.1",
    "jimp": "0.16.1",
    "joi": "17.4.0",
    "jwt-simple": "0.5.6",
    "lodash": "4.17.21",
    "moment": "2.29.3",
    "moment-range": "4.0.2",
    "morgan": "1.10.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6",
    "ts-node": "9.1.1",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@compodoc/compodoc": "1.1.11",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.15",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/qrcode": "1.4.0",
    "@types/request-ip": "0.0.37",
    "@types/socket.io": "2.1.13",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cross-env": "7.0.3",
    "eslint": "7.23.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prisma": "3.7.0",
    "prisma-dbml-generator": "0.6.0",
    "run-script-webpack-plugin": "0.0.11",
    "supertest": "6.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "8.1.0",
    "tsconfig-paths": "3.9.0",
    "webpack": "5.37.1",
    "webpack-node-externals": "2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add .",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lookFileMaintenance": {
    "matchDepTypes": [
      "devDependencies"
    ],
    "matchPackagePatterns": [
      "lint",
      "prettier"
    ],
    "automerge": true
  }
}
